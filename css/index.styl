form
  width 500px
  display block
  margin 0 auto
  border solid 1px #f0f0f0
  border-radius 5px
  label
    width 80px;
    display inline-block
  input[type="text"],select
    width 200px
  >div
    margin 5px 0
    border-bottom solid 1px #d0d0d0
    padding 10px 10px
    &:last-child
      border 0
.ckbox-group
  label
    margin-right 10px
    cursor pointer

